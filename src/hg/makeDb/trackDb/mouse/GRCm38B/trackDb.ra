# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
html gap

track chainNetMm10
compositeTrack on
shortLabel $o_Organism Chain/Net
longLabel $o_Organism ($o_date), Chain and Net Alignments
subGroup1 view Views chain=Chain net=Net psl=PSL
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 370.3
color 0,0,0
altColor 100,50,0
type bed 3
sortOrder view=+
matrix 16 91,-114,-31,-123,-114,100,-125,-31,-31,-125,100,-114,-123,-31,-114,91
matrixHeader A, C, G, T
otherDb mm10
chainMinScore 3000
chainLinearGap medium
spectrum on
html chainNet

    track chainNetMm10Viewchain
    shortLabel Chain
    view chain
    visibility pack
    parent chainNetMm10
    spectrum on

        track chainMm10
        parent chainNetMm10Viewchain
        subGroups view=chain
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain mm10
        html chainNet

    track chainNetMm10Viewnet
    shortLabel Net
    view net
    visibility full
    parent chainNetMm10

        track netMm10
        parent chainNetMm10Viewnet
        subGroups view=net
        shortLabel $o_Organism Net
        longLabel $o_Organism ($o_date) Alignment Net
        type netAlign mm10 chainMm10
        html chainNet

    track pslChainNetMm10Viewnet
    shortLabel PSL
    view psl
    visibility pack
    parent chainNetMm10

	track pslChainMm10
        parent pslChainNetMm10Viewnet
	shortLabel Mm10 PSL liftOver
	longLabel Mm10 liftOver chain viewed as PSL data
        subGroups view=psl
	type psl
	indelDoubleInsert on
	indelQueryInsert on
	showDiffBasesAllScales .
	showDiffBasesMaxZoom 10000.0
	showCdsMaxZoom 10000.0
	showCdsAllScales .
	color 0,0,100
	visibility pack

