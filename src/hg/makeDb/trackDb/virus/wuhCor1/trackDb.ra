# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
visibility hide
html gap.html

track rmsk override
visibility hide

searchTable gold
shortCircuit 1
termRegex NC_[0-9v]+
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s%%'
searchPriority 8

track ncbiGeneBGP
shortLabel NCBI Genes
longLabel NCBI Genes from GCF_009858895.2
group genes
visibility hide
priority 1
color 12,12,120
type bigGenePred 
bigDataUrl /gbdb/wuhCor1/ncbiGene.bb
baseColorUseCds given
baseColorDefault genomicCodons
labelFields geneName2,geneName,uniProtGene
url https://www.ncbi.nlm.nih.gov/protein/$$
urlLabel NCBI Refseq protein
exonNumbers off

track primers
shortLabel Detection Primers
longLabel Detection Kit Primer Sets
group map
type psl

searchName ncbiGene
searchTable ncbiGene
searchType genePred
searchMethod fuzzy
searchPriority 18

include trackDb.119way.ra
include epitopes.ra
include trackDb.44way.ra
include trackDb.strainName44way.ra
include trackDb.uniprot.ra
