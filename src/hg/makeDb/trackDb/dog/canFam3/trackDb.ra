include trackDb.chainNet.ra

# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
html gap

searchTable geneid
searchType genePred
searchMethod prefix
termRegex chr[0-9MUXn]+[AEJHX0-9_]+(\.[0-9]+)?
searchPriority 50

include ../../refSeqComposite.ra

track evaSnp
shortLabel EVA SNP Release 3
longLabel Short Genetic Variants from European Variant Archive Release 3
type bigBed 6 +
group varRep
visibility pack
bigDataUrl /gbdb/canFam3/bbi/evaSnp.bb
url https://www.ebi.ac.uk/eva/?variant&accessionID=$$
filterValues.valid No,Yes
filterValues.class deletion,delins,insertion,multipleNucleotideSubstitution,substitution,tandemRepeat

searchTable evaSnp
searchType bigBed
searchMethod exact
padding 50



